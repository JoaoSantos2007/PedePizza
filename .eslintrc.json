{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb-base", "import"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never"
        }
      ]
    }
}